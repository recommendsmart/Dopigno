{#
/**
 * @file basic_data.html.twig
 * Default theme implementation to present Basic Data data.
 *
 * This template is used when viewing Basic Data pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_basic_data()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('basic_data') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
